    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>UYARI!!!</h2>
<table class="table table-danger">
   <tr>
       <th>Sil</th>
       <th>İptal</th>
   </tr>

    @foreach (var item in Model)
