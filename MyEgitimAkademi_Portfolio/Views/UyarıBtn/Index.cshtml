        <tr>

            <td>@item.Title</td>
            <td>@item.Title</td>
            <td><a href="/Testimonial/UyarıBtnController/" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Testimonial/UyarıBtnController/" class="btn btn-outline-success">İptal</a></td>
        </tr>
    }

</table>
<a href="/Testimonial/UyarıBtnController/" class="btn btn-outline-danger">Uyarı</a>

